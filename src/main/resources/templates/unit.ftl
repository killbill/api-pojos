<#---------------------------------------------------------------------------->
<#import "common.ftl" as common>
<#---------------------------------------------------------------------------->
<#macro package name >
    <@common.block -8>
        package ${name};
    </@common.block>
</#macro>
<#---------------------------------------------------------------------------->
<#macro import types>
    <@common.block -12>
        <#list types as type>
            import ${type};
        </#list>
    </@common.block>
</#macro>
